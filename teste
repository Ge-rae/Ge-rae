#include <stdio.h>
#include <math.h>

int main(){

    int x1, x2, y1, y2;

    printf("digite x1: ");
    scanf("%d", &x1);
    printf("digite x2: ");
    scanf("%d", &x2);
    printf("digite y1: ");
    scanf("%d", &y1);
    printf("digite y2: ");
    scanf("%d", &y2);

    int d = sqrt(pow((x2 - x1), 2) + pow((y2 - y1), 2));

    printf("distancia: %d", d);
    return 0;
}
